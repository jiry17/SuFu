
Inductive List;

sum = fix (
    \f: List -> Int. \xs: List. 
    match xs with
      nil _ -> 0
    | cons {h, t} -> + h (f t)
    end
);

spec = \xs: List. 
    sum xs;

repr = fix (
    \f: List -> Int. \xs: List. 
    match xs with
      nil _ -> 0
    | cons {h, t} -> 
        let tmp2 = (f t) in 
            + h tmp2
    end
);

main = \xs: List. 
    let tmp3 = (repr xs) in 
        tmp3;
