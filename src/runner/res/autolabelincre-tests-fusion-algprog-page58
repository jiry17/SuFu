
Inductive List;

tri = \op: Int -> Int. 
    fix (
    \f: List -> Int. \xs: List. 
    match xs with
      nil _ -> 0
    | cons {h, t} -> 
        let tail = (let tmp3 = (f t) in 
            * tmp3 w) in 
            + h tail
    end
);

@Input w = Int;

op = \x: Int. 
    * x w;

sum = fix (
    \f: List -> Int. \xs: List. 
    match xs with
      nil _ -> 0
    | cons {h, t} -> + h (f t)
    end
);

main = \xs: List. 
    let tmp4 = (tri op xs) in 
        tmp4;
