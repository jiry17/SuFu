
Inductive List;

Inductive CList;

max = \a: Int. \b: Int. 
    if (< a b) then b
    else a;

@Input x = Int;

spec = fix (
    \f: List -> Bool. \xs: List. 
    match xs with
      nil _ -> false
    | cons {hd, tl} -> or (== x hd) (f tl)
    end
);

cat = fix (
    \f: List -> List -> List. \xs: List. \ys: List. 
    match xs with
      nil _ -> ys
    | cons {a, b} -> cons {a, f b ys}
    end
);

repr = fix (
    \f: CList -> Bool. \xs: CList. 
    match xs with
      cnil _ -> false
    | single a -> == a x
    | concat {a, b} -> 
        let tmp3 = (f a) in 
            let tmp4 = (f b) in 
                or tmp4 tmp3
    end
);

main = \x: CList. 
    let tmp5 = (repr x) in 
        tmp5;
