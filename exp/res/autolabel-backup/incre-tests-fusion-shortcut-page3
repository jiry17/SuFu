
Inductive List;

div2 = \x: Int. 
    / x 2;

from = \a: Int. \b: Int. 
    fix (
    \f: Int -> Int -> Int. \now: Int. \r: Int. 
    if (> now r) then 0
    else - (+ (+ now (div2 (- b now))) (div2 (+ b (* b b)))) (div2 (+ b (* now now)))) a b;

sum = fix (
    \f: List -> Int. \xs: List. 
    match xs with
      nil _ -> 0
    | cons {h, t} -> + h (f t)
    end
);

main = \a: Int. \b: Int. 
    if (<= a b) then let tmp3 = (from a b) in 
        tmp3
    else 0;
